{
  "name": "UniRequest",
  "version": "1.0.0",
  "description": "Universal Request Standard for Modern Web and Multi-Platform Apps",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "commit:check": "lavy commit --test",
    "commit:config": "lavy commit --config",
    "demo": "tsx src/example.ts"
  },
  "keywords": [],
  "author": "Gavin",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-lavy": "^2.0.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.12.0",
    "husky": "^9.1.0",
    "lavy": "^2.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.0",
    "swiftlet": "1.0.0-Alpha.4",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
